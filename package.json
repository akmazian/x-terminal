        "@types/react-router-dom": "5.3.3",
        "autoprefixer": "10.4.19",
        "eslint": "8.57.0",
        "eslint-config-next": "14.2.3",
        "ionicons": "7.4.0",
        "postcss": "8.4.38",
        "prettier": "3.3.1",
