        "@capacitor/keyboard": "6.0.0",
        "@capacitor/status-bar": "6.0.0",
        "@ionic/react": "8.2.1",
        "@ionic/react-router": "8.2.1",
        "@reduxjs/toolkit": "2.2.5",
        "@supabase/ssr": "0.3.0",
        "@supabase/supabase-js": "2.43.4",
        "classnames": "2.5.1",
